{
    "name": "firdows/yii2-mcms",
    "description": "CMS",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Ahamad Jehdu",
            "email": "ahamad.jedu@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/yii2": "*",
        "sunhater/kcfinder": "*",        
        "2amigos/yii2-ckeditor-widget": "~1.0"
    },
    "autoload": {
